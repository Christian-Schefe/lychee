struct Test {
    int a;
    long b;
}

unit pointer_test(&int pointer) {
    long c = 0x100000068;
    &long long_pointer = &c;
    &int casted_pointer = long_pointer as &int;
    *casted_pointer = 0x63;
    writechar((*casted_pointer) as char);
    writenum(c);
    *pointer += 125 * *pointer;
}

unit write_alphabet(&int counter) {
    for (*counter = 0; *counter < 26; (*counter)++) {
        writechar(((*counter += 2) + 'a' as int) as char);
    }
}

unit writenum(long num) {
    if num == 0 {
        writechar('0');
        return;
    } else if num < 0 {
        writechar('-');
        num = -num;
    }

    if num >= 10 {
        writenum(num / 10);
    }
    writechar((num % 10 + '0' as long) as char);
}
