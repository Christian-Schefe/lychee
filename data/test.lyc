     Point p1 = (Point) {
        x: 5,
        y: 88
    };
    p1 = add(p1, (Point) {
        x: 8,
        y: 5
    }, ());
    Point p2 = add(p1 = add(p1, p1, ()), p1, writenum(p1.y));
