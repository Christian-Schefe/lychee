    p2.x
}

struct Point {
    int x;
    int y;
}

unit call_order_test(int a, long b, bool c) {
    writenum(a);
    writenum(b as int);
    writenum(c as int);
}

Point add(Point p1, Point p2, unit a) {
    return (Point) {
        x: p1.x + p2.x,
        y: p1.y + p2.y
    };
}

unit writenum(int num) {
    if num == 0 {
        writechar('0');
        return;
    } else if num < 0 {
        writechar('-');
        num = -num;
    }

    if num >= 10 {
        writenum(num / 10);
    }
    writechar((num % 10 + '0' as int) as char);
}
