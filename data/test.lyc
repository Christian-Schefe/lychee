unit writenum(int num) {
    if num == 0 {
        writechar('0');
        return;
    } else if num < 0 {
        writechar('-');
        num = -num;
    }

    if num >= 10 {
        writenum(num / 10);
    }
    writechar((char)(num % 10 + (int)'0'));
}

unit pointer_test(&int a) {
    *a = 123;
    writenum(*a);
    writechar('\n');
}
